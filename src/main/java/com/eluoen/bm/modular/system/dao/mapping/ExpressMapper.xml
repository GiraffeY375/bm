<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.eluoen.bm.modular.system.dao.ExpressMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.eluoen.bm.modular.system.model.Express">
        <id column="id" property="id" />
        <result column="exp_code" property="expCode" />
        <result column="exp_name" property="expName" />
        <result column="logo" property="logo" />
        <result column="tel" property="tel" />
        <result column="status" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, exp_code AS expCode, exp_name AS expName, logo, tel, status
    </sql>

</mapper>
